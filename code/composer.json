{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "laravel/framework": "4.2.*",
        "rcrowe/twigbridge": "0.6.*@dev",
        "stolz/assets": "dev-master",
        "components/jquery": "1.11.*@dev",
        "twbs/bootstrap": "3.2.*",
        "components/font-awesome": "4.1.*@dev",
        "afarkas/html5shiv": "dev-master",
        "rogeriopradoj/respond": "dev-composer",
        "kwipi/famfamfam-flags": "dev-master"
    },
    "autoload": {
        "classmap": [
            "app/commands",
            "app/controllers",
            "app/models",
            "app/repositories",
            "app/database/migrations",
            "app/database/seeds",
            "app/tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "stable"
}
